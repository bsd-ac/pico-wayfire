gsettings set org.gnome.desktop.interface gtk-theme Sweet || \
    pwarn "Could not change gtk-theme"
gsettings set org.gnome.desktop.interface icon-theme PicoWayfire || \
    pwarn "Could not change icon theme"
gsettings set org.gnome.desktop.interface cursor-theme Bibata-Modern-Classic || \
    pwarn "Could not change cursor theme"
